{
  "name": "react-fe",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "fmt": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss}'",
    "stylelint": "stylelint 'src/**/*.{css,ts,tsx}' --max-warnings 0",
    "preview": "vite preview",
    "lint:precommit": "eslint 'src/**/*.{ts,tsx} --fix'",
    "fmt:precommit": "prettier './**/*.{js,jsx,ts,tsx}' --write"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": "npm eslint --fix",
    "src/**/*.{js,jsx,ts,tsx,json,css,scss}": "npm run fmt:precommit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@emotion/eslint-plugin": "^11.11.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-react": "^4.0.3",
    "eslint": "^8.45.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.1",
    "stylelint": "^15.10.2",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-order": "^6.0.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
}
